<beans>
    <bean name="Skoda Dealer" class="com.upt.cbse.test.dealership.Dealer">
        <property name="name" value="Skoda"/>
    </bean>

    <bean name="VW Dealer" class="com.upt.cbse.test.dealership.Dealer">
        <property name="name" value="VW"/>
    </bean>

    <bean name="person" class="com.upt.cbse.test.dealership.Person">
        <property name="firstName" value="Bogdan"/>
        <property name="lastName" value="Pocol"/>
    </bean>

    <bean name="storeManager" class="com.upt.cbse.test.dealership.Manager">
        <property name="person" ref="person"/>
        <property name="dealer" ref="Skoda Dealer"/>
    </bean>
</beans>
